#include<iostream>
using namespace std;

bool isprime(int n)
{
   if(n<2)
   {
    return 0;
   }
   else if (n==2)
   {
    return 1;
   }
   else if(n%2==0)
   {
    return 0;
   }
   else
   {
    for (int i=3;i<n;i=i+2)
    {
    if (n%i==0)
    {
    return 0;
}
   }
   return 1;
   }
   
   }
int main()
{
for (int j=2;j<100;j++)
 {
  if  (isprime(j)==1)
  {
  cout<< j <<"is prime."<<endl;
}
 }
}

   
   
   

   
